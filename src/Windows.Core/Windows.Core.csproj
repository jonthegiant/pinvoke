    <TargetFrameworks>portable-net4+win8+wpa81</TargetFrameworks>
    <Summary>P/Invoke types for common Windows APIs.</Summary>
    <Description>P/Invoke types for common Windows APIs.</Description>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Roslyn.Diagnostics.Analyzers" Version="1.2.0-beta2" PrivateAssets="all" />
